<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:identity="urn:import:org.openstack.ui.client.view.identity">
	<ui:style>
		.compute {
			margin: 10px;
		}
		
		.nav {
			border-right: 4px solid #E7E7E7;
			height: 100%;
			background-color: #F1F1F1;
		}
		
		.content {
			border: 4px solid #E7E7E7;
		}
		
		.m5 {
			margin: 5px;
		}
		
		.m10 {
			margin: 10px;
		}
		
		.top-inner {
			table-layout: fixed;
			width: 100%;
		}
		
		.tt {
			border-collapse: separate;
			border-spacing: 5px;
		}
		
		.w100 {
			width: 100%;
		}
	</ui:style>
	<g:DockLayoutPanel styleName="{style.content}">
		<g:west size="180">
			<g:HTMLPanel styleName="{style.nav}">
				<identity:IdentityNavigation />
			</g:HTMLPanel>
		</g:west>
		<g:center>
			<g:SimpleLayoutPanel ui:field="content" />
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder> 